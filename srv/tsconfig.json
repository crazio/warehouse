{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "types": [
      "reflect-metadata"
    ],
    "noEmit": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": false,
    "noEmitOnError": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src-ts"
  ]
}